module Memoria(
	input [31:0] w_PC,
	output [31:0] w_Inst,
);

	always(*)begin
		case(w_PC)
		32'h00: w_Inst = 00001111001100000000000010010011;
		32'h04: w_Inst = 00000000100100000000000100110011;
		32'h08: w_Inst = 00000000001000001000000100110011;
		32'h0C: w_Inst = 00000000001000001111000110110011;
		32'h10: w_Inst = 00000000001000001110001000110011;
		32'h14: w_Inst = 00000000010000001100001010110011;
		32'h18: w_Inst = 00000000010000011010001100110011;
		32'h1C: w_Inst = 00000000011000100000001110110011;
		default: w_Inst = 32'h0;
	end
endmodule
